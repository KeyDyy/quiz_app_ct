{
  "version": "2.0",
  "customHandler": {
    "description": {
      "defaultExecutablePath": "python",
      "workingDirectory": "",
      "arguments": [
        "-m",
        "azure.functions"
      ]
    },
    "enableForwardingHttpRequest": true
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "functionTimeout": "00:10:00",
  "managedDependency": {
    "enabled": true
  },
  "applicationInsights": {
    "samplingSettings": {
      "isEnabled": true,
      "excludedTypes": "Request"
    }
  }
}